<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jeff.vrms">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-feature android:name="android.hardware.camera"
        android:required="true" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/icke"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icke"
        android:supportsRtl="true"
        android:theme="@style/Theme.VRMS">
        <activity
            android:name=".ui.Details"
            android:exported="true" />
        <activity
            android:name=".ui.SignUp"
            android:exported="true" />
        <activity
            android:name=".ui.Verification"
            android:exported="true" />
        <activity
            android:name=".ui.Registration"
            android:exported="true" />
        <activity
            android:name=".ui.Login"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>